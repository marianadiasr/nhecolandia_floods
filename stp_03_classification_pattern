// 'water', FeatureCollection, class:1, color '#04dcff
// 'wet soil', FeatureCollection, class:2, color '#ce5430
// 'exposed soil', FeatureCollection, class:3, color '#ffc82d
// 'vegetation', FeatureCollection, class:4, color '#159b0c

//4 - Randomization process and training of samples

var waterPoints = image.sampleRegions(water,null,3,null,1,true)
  .randomColumn('random').sort('random').limit(500);

Map.addLayer(waterPoints, {}, 'Water Points');

var wetSoilPoints = image.sampleRegions(vazante,null,3,null,1,true)
  .randomColumn('random').sort('random').limit(500)
Map.addLayer(wetSoilPoints, {}, 'Wet Soil Points')

var drySoilPoints = image.sampleRegions(exposedsoil,null,3,null,1,true)
  .randomColumn('random').sort('random').limit(500)
Map.addLayer(drySoilPoints, {}, 'Dry Soil Points')

var vegetationPoints = image.sampleRegions(cordilheira,null,3,null,1,true)
  .randomColumn('random').sort('random').limit(500)
Map.addLayer(vegetationPoints, {}, 'Vegetation Points');

var dataSet = waterPoints.merge(vegetationPoints.merge(wetSoilPoints.merge(drySoilPoints)))

//better understand these commands
var randomizedData = dataSet.randomColumn('randomPreClassification')
var trainingData = randomizedData.filter(
  ee.Filter.lte('randomPreClassification',0.7))
var testingData = randomizedData.filter(
  ee.Filter.gt('randomPreClassification',0.7))
  
var classifier = ee.Classifier.smileRandomForest(100) 
var trainedClassifier = classifier.train({
  features: trainingData,
  classProperty: 'class',
  inputProperties: bands
});

// Display classification (standard)
var classifiedImage = image.classify(trainedClassifier)
Map.addLayer(classifiedImage,{min:1,max:4, palette:['04dcff','ce5430','ffc82d','159b0c']},'Classified Image')

//6 - Validation and Accuracy Processes
var validation = testingData.classify(trainedClassifier)
var confusionMatrix = validation.errorMatrix('class','classification')
print(confusionMatrix)

print('Overall Accuracy', confusionMatrix.accuracy())
print('Producer’s Accuracy', confusionMatrix.producersAccuracy())
print('Consumer’s Accuracy', confusionMatrix.consumersAccuracy())


Export.image.toAsset({
  image: classifiedImage,
  description: 'A2_180424',
  assetId: 'your export ID here',
  region: classifiedImage,
  scale: 3,
  maxPixels: 1e13
})

// Capture the values of the confusion matrix
var matrixArray = ee.Array(confusionMatrix.array());
print('Matrix as array:', matrixArray);
var image_id = 'A2_180424'
// Save as Feature with metadata (image ID, date, etc.)
var matrixFeature = ee.Feature(null, {
  'matrix': matrixArray,
  'image_id': image_id,  // <- change according to each image ID
  'area': 'A2',
  'date': ee.Date(image.get('system:time_start')).format('YYYY-MM-dd')
});

// Create a FeatureCollection with a single row
var matrixCollection = ee.FeatureCollection([matrixFeature]);

// Export as table to Drive (CSV format)
Export.table.toDrive({
  collection: matrixCollection,
  description: 'ConfusionMatrix_'+image_id,
  folder: '',
  fileFormat: 'CSV'
});
